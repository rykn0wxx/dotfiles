[global_config]
  title_hide_sizetext = True
  title_transmit_fg_color = "#01a299"
  title_transmit_bg_color = "#050709"
[keybindings]
[profiles]
  [[default]]
    background_color = "#121212"
    background_darkness = 0.9
    background_type = transparent
    cursor_shape = ibeam
    cursor_color = "#aaaaaa"
    font = Fira Code 10
    foreground_color = "#dd4132"
    show_titlebar = False
    scrollback_lines = 10000
    palette = "#00aaff:#cc0000:#4e9a06:#c4a000:#3465a4:#75507b:#06989a:#d3d7cf:#555753:#ef2929:#8ae234:#fce94f:#729fcf:#ad7fa8:#34e2e2:#eeeeec"
    login_shell = True
    use_system_font = False
    copy_on_selection = True
[layouts]
  [[default]]
    [[[window0]]]
      type = Window
      parent = ""
    [[[child1]]]
      type = Terminal
      parent = window0
[plugins]
